        <dependency id="Shuttle.Core.Configuration" version="{Shuttle.Core.Configuration-version}" />
        <dependency id="Shuttle.Core.Data" version="{Shuttle.Core.Data-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
