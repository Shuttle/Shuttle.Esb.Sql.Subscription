<?xml version="1.0"?>

<package>
  <metadata>
    <id>Shuttle.Esb.Sql.Subscription</id>
    <version>{semver}</version>
    <authors>Eben Roux</authors>
    <owners>Eben Roux</owners>
    <licenseUrl>https://github.com/shuttle/Shuttle.Esb.Sql.Subscription/raw/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/shuttle/Shuttle.Esb.Sql.Subscription</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Shuttle/Shuttle.Esb.Sql.Subscription/master/.media/logo.png</iconUrl>
